game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Loading source", -- Required
	Text = "Please Wait...", -- Required
    Duration = 5
})

if game.PlaceId == 286090429 then
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "Source Arsenal", -- Required
        Text = "Game Supported! Loading Menu... (Arsenal)", -- Required
        Duration = 5
    })

    loadstring(game:HttpGet("https://raw.githubusercontent.com/sourcemainv2/arcenalFreetrail/refs/heads/main/v2"))()
else
    game:GetService("StarterGui"):SetCore("SendNotification",{
        Title = "Load Failed!", -- Required
        Text = "Source does not support this place!", -- Required
        Duration = 5
    })
end
